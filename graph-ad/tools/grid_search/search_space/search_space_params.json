{
  "dataset": {
    "name": "Darpa",
    "filename": "C:\\Users\\kfirs\\lab\\TPGAD\\data\\darpa.csv",
    "directed": true,
    "time_format": "%m/%d/%Y-%H:%M",
    "time_col": "time",
    "src_col": "src",
    "dst_col": "dst",
    "weight_col": null,
    "week_split": 0,
    "day_split": 0,
    "hour_split": 1,
    "min_split": 0,
    "sec_split": 0
